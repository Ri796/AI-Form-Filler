{
  "manifest_version": 3,
  "name": "AI Form Filler",
  "version": "0.1.0",
  "description": "A smart agent to automatically fill online forms.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}